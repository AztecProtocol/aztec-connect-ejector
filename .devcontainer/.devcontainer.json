{
  "name": "Aztec V2 ejector",
  "hostRequirements": {
    "cpus": 16,
    "memory": "64gb",
    "storage": "32gb"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ],
      "settings": {
        "github.codespaces.defaultIdleTimeout": 180 // 3 hours
      }
    }
  }
}
